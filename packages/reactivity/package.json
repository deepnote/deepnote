{
  "name": "@deepnote/reactivity",
  "version": "0.1.0",
  "description": "Reactivity and dependency graph for Deepnote notebooks",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./browser": {
      "import": "./dist/browser.js",
      "require": "./dist/browser.cjs",
      "types": "./dist/browser.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown src/index.ts src/browser.ts --format cjs,esm --dts --clean",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "zod": "3.25.76"
  },
  "engines": {
    "node": ">=22.14.0",
    "pnpm": ">=10.17.1"
  },
  "license": "MIT"
}
